{
  "name": "eslint-config-silence",
  "version": "2.0.0",
  "description": "Eslint config for code-quality, prevent possible errors and follow best practices",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "prettier",
    "check",
    "code checker",
    "lint",
    "code linter",
    "quality",
    "verify"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=15",
    "npm": ">=7"
  },
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:audit": "npm audit --audit-level=high",
    "test:lint": "eslint . '**/*.{js,ts}'",
    "test:unit": "tape test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zapolnoch/eslint-config-silence.git"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-sonarjs": "0.12.0",
    "eslint-plugin-unicorn": "41.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tape": "^5.5.2",
    "typescript": "4.5.5"
  }
}
